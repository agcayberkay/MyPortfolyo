
@model List<MyPortfolyo.DAL.Entities.Skils>



    <div class="col-six tab-full right">
        <h3>Yeteneklerim</h3>

        <ul class="skill-bars">
        @foreach (var item in Model)
        {
            <li>
                <div class="progress" style="width:@item.Value%"><span>@item.Value</span></div>
                <strong>@item.Title</strong>
            </li>
        }   
        </ul>
    </div>


